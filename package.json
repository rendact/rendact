    "react-addons-shallow-compare": "^15.6.0",
    "react-addons-update": "^15.6.0",
    "react-apollo": "^1.0.0",
    "react-bootstrap": ">=0.30.2",
    "react-bootstrap-date-picker": "^5.1.0",
    "react-bootstrap-timezone-picker": ">=1.0.10",
