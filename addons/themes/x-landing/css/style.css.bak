/*-- sticky-nav --*/
.fixed{
  position: fixed;
  top: 0;
  width: 100%;
  margin: 0 auto;
  left:0;
}
.top-menu ul{
  padding:0;
}
.top-menu ul li{
  display:inline-block;
  list-style:none;
  margin: 0 4px;
}
.top-menu ul li a{
  color: #fff;
  font-size: 1em;
  padding: 15px 15px;
  font-weight:600;
  text-decoration: none;
  text-transform: uppercase;
  border-top: 1px solid rgba(39, 40, 40, 0.09);
  background: rgba(47, 48, 48, 0);
}
.top-menu ul li a.active,.top-menu ul li a:hover{
  color:#698a9a;
  border-top:1px solid #698a9a;transition:0.5s all;
  -webkit-transition:0.5s all;
  -moz-transition:0.5s all;
  -o-transition:0.5s all;
  -ms-transition:0.5s all;
   background-color: rgba(39, 40, 40, 0.74);
}

.top-menu {
  float: right;
  margin-top: 1em;
}
.header-bottom {
  padding: 1.5em 0;
  z-index: 999;
  background-color: rgba(39, 40, 40, 0.9);
}
nav.navigation a {
  display: block;
  margin: 0px 0px;
  padding: 0;
  text-decoration: none;
}
nav.navigation a:hover,
nav.navigation a:focus {
  outline: none;
}

/* dropdown menu slide down 1 */

.dropdown:hover >  .dropdown-menu{
    -webkit-transform:scale(1, 1);
    -moz-transform: scale(1, 1);
    -o-transform:scale(1, 1);
    transform:scale(1, 1);
}

.dropdown-menu {
    padding:0;
    border:none;
    -webkit-transform:scale(1, 0);
    -moz-transform: scale(1, 0);
    -o-transform:scale(1, 0);
    transform:scale(1, 0);

    -webkit-transform-origin: top;
    -moz-transform-origin: top;
    -o-transform-origin: top;
    transform-origin: top;

    -webkit-animation-fill-mode: forwards;  
    -moz-transform-origin: forwards;
    -o-transform-origin: forwards;
    animation-fill-mode: forwards; 

    -webkit-transition: all 200ms ease-in;
    -moz-transition: all 200ms ease-in;
    -o-transition: all 200ms ease-in;
    transition: all 200ms ease-in;
    margin: 0;
    display: block;
}

.dropdown-menu .dropdown a {
    white-space: normal;
}

/*-- sticky-nav --*/
@media screen and (max-width: 700px) {
    span.menu{
    width:35px;
    height:35px;
    background:url('../images/menu-icon.png') no-repeat;
    display: inline-block;
    float:right;
    cursor: pointer;
    margin-top: 12px;
  }
  .header-bottom {
      padding: 1em 0;
  }
  .top-menu {
        width: 100%;
      display: none;
      text-align: center;
      background: rgba(13, 13, 13, 0.62);
      padding: 0;
      margin-top: 14px;
  }
  .top-menu ul{
    float:none;
    width:100%;
  }
  .top-menu ul li{
    display:block;
    float: none;
  }
  .top-menu ul li a {
      color: #fff;
      display: block;
      padding: 10px 0px;
      margin: 5px 0;
  }
}
