.content-body {
  padding-top: 2em;
}
.sidebar-grid {
  margin-left: 2em;
}
.sidebar-grid h3{
  color: #fff;
  font-size: 1.2em;
  text-align:left;
  margin-bottom: 6%;
  text-transform: uppercase;
  font-family: 'Arial', sans-serif;
}
.sidebar-grid p, .sidebar-grid a {
  text-align:left;
  font-size: 0.9em;
  line-height: 1.7em;
  color: #1b1b1b;
  font-weight: 400;
}
.sidebar-box {
  padding-bottom: 2em;
  text-align:left;
}
.sidebar-box ul li {
  list-style:none;
  display:block;
}
.sidebar-box ul li a {
  font-size:0.9em;
  margin: 0 0 1em 0;
   color: #1b1b1b;
  line-height: 1.8em;
  text-decoration: none;
  padding-left: 2em;
  background: url(../images/arrow.png) no-repeat 0px 6px;
}
.sidebar-box ul li a:hover {
  padding-left: 3em;
}
.sidebar-box > ul.posts-list li a {
  margin: 0;
  line-height: inherit;
  text-decoration: none;
  padding: 0;
  background: none;
}
.sidebar-box > ul.posts-list li {
  padding-bottom: 1em;
}
.sidebar-box h3 {
  color: #555;
}
.sidebar-box h5 {
  color: #698a9a;
  font-weight: 500;
  font-size: 1.3em;
}

.sidebar-box a h5:hover {
  color: #111111;
}
